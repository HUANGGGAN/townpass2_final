{
  "name": "townpass-backend",
  "version": "1.0.0",
  "description": "TownPass Backend - Safety Retrospection System",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "prestart": "npm run build && tsx scripts/init.ts",
    "start": "node dist/server.js",
    "init": "tsx scripts/init.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "seed": "tsx prisma/seeds/index.ts",
    "seed:reset": "tsx prisma/seeds/reset.seed.ts",
    "reset:seed": "tsx prisma/seeds/resetAndSeed.ts",
    "seed:identity": "tsx prisma/seeds/identity.seed.ts",
    "seed:safe-place": "tsx prisma/seeds/safePlace.seed.ts",
    "seed:cctv": "tsx prisma/seeds/cctv.seed.ts",
    "seed:not-safe": "tsx prisma/seeds/notSafe.seed.ts",
    "seed:grids": "tsx prisma/seeds/grids.seed.ts",
    "seed:places": "tsx prisma/seeds/places.seed.ts",
    "seed:danger-points": "tsx prisma/seeds/dangerPoints.seed.ts",
    "seed:danger-points-random": "tsx prisma/seeds/dangerPointsRandom.seed.ts",
    "seed:danger": "tsx prisma/seeds/dangerPointsRandom.seed.ts",
    "lint": "eslint src --ext .ts",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prisma": "^5.7.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
